        "react-dom": "^16.14.0",
        "react-helmet": "^6.1.0",
        "react-icons": "^4.2.0",
        "react-select": "^3.1.0",
        "react-toast-notifications": "^2.4.0",
        "styled-components": "^5.2.0",
        "use-query-params": "^1.1.8",
        "uuid": "^8.3.1",
        "validator": "^13.1.17"
